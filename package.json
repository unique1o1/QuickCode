{
  "name": "quickcode",
  "displayName": "quickCode",
  "description": "Quickly paste code from Stackoverflow",
  "version": "0.8.0",
  "publisher": "yunikmaha",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Other",
    "Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.quickcode"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.quickcode",
        "title": "QuickCode: Search StackOverFlow"
      }
    ],
    "keybindings": [
      {
        "command": "extension.quickcode",
        "key": "Ctrl+Shift+Q",
        "mac": "Cmd+Shift+Q"
      }
    ]
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#AF47C1",
    "theme": "dark"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.0.64",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^4.19.1",
    "typescript": "^2.8.3",
    "vscode": "^1.1.17"
  }
}
