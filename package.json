{
  "name": "quickcode",
  "displayName": "quickCode",
  "description": "Quickly paste code from Stackoverflow",
  "version": "0.6.0",
  "publisher": "yunikmaha",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": ["Other", "Languages", "Snippets"],
  "activationEvents": ["onCommand:extension.quickcode"],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.quickcode",
        "title": "QuickCode: Search StackOverFlow"
      }
    ],
    "keybindings": [
      {
        "command": "extension.quickcode",

        "key": "Ctrl+Shift+Q",
        "mac": "Cmd+Shift+Q"
      }
    ]
  },

  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#AF47C1",
    "theme": "dark"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^4.6.1",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}
